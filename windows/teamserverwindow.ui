   <string notr="true">/* === Global Base === */
* {
    font-family: &quot;Consolas&quot;, &quot;JetBrains Mono&quot;, &quot;monospace&quot;;
    font-size: 11pt;
    color: #dcdcdc;
    background: none;
}

/* === Main Background === */
QMainWindow, QWidget {
    background-color: #1e1e1e;
    border: none;
}

/* === QMenuBar === */
QMenuBar {
    background-color: #252526;
    color: #dcdcdc;
    padding: 4px;
}
QMenuBar::item {
    padding: 6px 12px;
}
QMenuBar::item:selected {
    background-color: #007acc;
    color: white;
}

/* === QMenu === */
QMenu {
    background-color: #2d2d30;
    color: #dcdcdc;
    border: 1px solid #3c3c3c;
}
QMenu::item:selected {
    background-color: #007acc;
    color: white;
}

/* === QPushButton === */
QPushButton {
    background-color: #3c3c3c;
    border: 1px solid #555;
    padding: 6px 12px;
    color: #dcdcdc;
}
QPushButton:hover {
    background-color: #444;
}
QPushButton:pressed {
    background-color: #222;
}
QPushButton:disabled {
    background-color: #2a2a2a;
    color: #777;
    border-color: #444;
}

/* === QLineEdit / QTextEdit === */
QLineEdit, QTextEdit {
    background-color: #2a2a2a;
    border: 1px solid #555;
    color: #ffffff;
    selection-background-color: #007acc;
    selection-color: #ffffff;
    padding: 4px;
}

/* === QPlainTextEdit (e.g., console output) === */
QPlainTextEdit {
    background-color: #2a2a2a;
    border: 1px solid #555;
    color: #ffffff;
    selection-background-color: #007acc;
    selection-color: #ffffff;
}

/* === QTableWidget / QTableView === */
QTableWidget, QTableView {
    background-color: #262626;
    color: #ffffff;
    border: 1px solid #444;
    gridline-color: #666;
    selection-background-color: #007acc;
    selection-color: #ffffff;
    alternate-background-color: #1e1e1e;
}
QHeaderView::section {
    background-color: #333;
    color: #dcdcdc;
    border: 1px solid #444;
    padding: 4px;
}

/* === QListWidget / QListView === */
QListWidget, QListView {
    background-color: #262626;
    border: 1px solid #444;
    color: #ffffff;
}
QListWidget::item:selected, QListView::item:selected {
    background-color: #007acc;
    color: #ffffff;
}

/* === QTabWidget === */
QTabWidget::pane {
    border: 1px solid #3c3c3c;
    padding: 4px;
}
QTabBar::tab {
    background-color: #2d2d30;
    color: #dcdcdc;
    padding: 6px 12px;
    border: 1px solid #3c3c3c;
    border-bottom: none;
}
QTabBar::tab:selected {
    background-color: #1e1e1e;
    color: white;
    border: 1px solid #007acc;
    border-bottom: none;
}
QTabBar::tab:hover {
    background-color: #383838;
}

/* === QToolBar === */
QToolBar {
    background-color: #2d2d30;
    border-bottom: 1px solid #3c3c3c;
    spacing: 6px;
}
QToolButton {
    background-color: #2d2d30;
    border: none;
    padding: 6px;
    margin: 2px;
    color: #dcdcdc;
}
QToolButton:hover {
    background-color: #3a3a3a;
}
QToolButton:pressed {
    background-color: #007acc;
    color: #ffffff;
}

/* === QScrollBar === */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #1e1e1e;
    border: none;
    margin: 0px;
    width: 12px;
    height: 12px;
}
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #555;
    min-height: 20px;
    min-width: 20px;
    border-radius: 6px;
}
QScrollBar::handle:hover {
    background: #007acc;
}
QScrollBar::add-line, QScrollBar::sub-line {
    height: 0px;
    width: 0px;
}
QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}

/* === QCheckBox === */
QCheckBox {
    color: #dcdcdc;
}
QCheckBox::indicator {
    width: 14px;
    height: 14px;
}
QCheckBox::indicator:unchecked {
    border: 1px solid #555;
    background-color: #2a2a2a;
}
QCheckBox::indicator:checked {
    background-color: #007acc;
    border: 1px solid #007acc;
}

/* === QComboBox === */
QComboBox {
    background-color: #2a2a2a;
    border: 1px solid #555;
    padding: 4px;
    color: #ffffff;
}
QComboBox:hover {
    border: 1px solid #007acc;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #555;
    background-color: #2a2a2a;
}
QComboBox QAbstractItemView {
    background-color: #262626;
    color: #ffffff;
    selection-background-color: #007acc;
    selection-color: #ffffff;
}

/* === QGroupBox === */
QGroupBox {
    border: 1px solid #3c3c3c;
    margin-top: 20px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 4px;
    color: #dcdcdc;
}

