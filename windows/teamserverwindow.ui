QListWidget {
    background-color: #121826;
    border: 1px solid #4C8EDA;
    color: #E6F1FF;
}

QListWidget::item {
    padding: 4px;
}

QListWidget::item:selected {
    background-color: #264F78;
    color: #FFFFFF;
}

/* SCROLLBARS */
